{
    "name": "DipityTest",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Randomly inserting articles from your read-it-later services into your FB feed.",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": "icons/icon19.png",
        "default_title": "page action demo",
        "default_popup": "src/popup.html"
    },
    "options_page": "src/page_action/page_action.html#settings",
    "content_scripts": [{
        "matches": [
            "https://www.facebook.com/*"
        ],
        "css": [
            "src/inject/inject.css"
        ],
        "js": [
            "src/inject/inject.js"
        ]
    }],
    "permissions": [
        "storage",
        "https://*.getpocket.com/*"
    ],
    "web_accessible_resources": [
        "src/auth_complete.html"
    ],
    "background": {
        "scripts": ["js/jquery-1.8.3.min.js", "js/config.js", "js/auth_listener.js"]
    }
}